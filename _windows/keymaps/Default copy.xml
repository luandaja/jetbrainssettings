<keymap version="1" name="Default copy" parent="$default">
  <action id="$Redo">
    <keyboard-shortcut first-keystroke="shift ctrl z" />
    <keyboard-shortcut first-keystroke="shift alt back_space" />
    <keyboard-shortcut first-keystroke="ctrl y" />
  </action>
  <action id="$SearchWeb">
    <keyboard-shortcut first-keystroke="ctrl alt g" />
  </action>
  <action id="Editor Redo">
    <keyboard-shortcut first-keystroke="ctrl y" />
  </action>
  <action id="Editor SearchWeb">
    <keyboard-shortcut first-keystroke="ctrl alt g" />
  </action>
  <action id="EditorCodeBlockEnd">
    <keyboard-shortcut first-keystroke="ctrl braceright" />
  </action>
  <action id="EditorCodeBlockEndWithSelection">
    <keyboard-shortcut first-keystroke="shift ctrl braceright" />
  </action>
  <action id="EditorCodeBlockStart">
    <keyboard-shortcut first-keystroke="ctrl braceleft" />
  </action>
  <action id="EditorCodeBlockStartWithSelection">
    <keyboard-shortcut first-keystroke="shift ctrl braceleft" />
  </action>
  <action id="EditorDecreaseFontSize">
    <keyboard-shortcut first-keystroke="alt subtract" />
  </action>
  <action id="EditorDeleteLine">
    <keyboard-shortcut first-keystroke="alt y" />
  </action>
  <action id="EditorIncreaseFontSize">
    <keyboard-shortcut first-keystroke="alt add" />
  </action>
  <action id="EditorResetFontSize">
    <keyboard-shortcut first-keystroke="alt numpad0" />
  </action>
  <action id="FileChooser.TogglePathShowing">
    <keyboard-shortcut first-keystroke="shift p" />
  </action>
  <action id="GotoDeclaration">
    <keyboard-shortcut first-keystroke="ctrl b" />
    <mouse-shortcut keystroke="control button1" />
    <mouse-shortcut keystroke="button2" />
    <keyboard-shortcut first-keystroke="f12" />
  </action>
  <action id="JumpToLastWindow">
    <keyboard-shortcut first-keystroke="shift alt f12" />
  </action>
  <action id="ParameterInfo">
    <keyboard-shortcut first-keystroke="alt p" />
  </action>
  <action id="SearchEverywhere">
    <keyboard-shortcut first-keystroke="ctrl p" />
  </action>
  <action id="SwitchApply">
    <keyboard-shortcut first-keystroke="shift alt enter" />
  </action>
  <action id="SwitchDown">
    <keyboard-shortcut first-keystroke="shift alt down" />
  </action>
  <action id="SwitchLeft">
    <keyboard-shortcut first-keystroke="shift alt left" />
  </action>
  <action id="SwitchRight">
    <keyboard-shortcut first-keystroke="shift alt right" />
  </action>
  <action id="SwitchUp">
    <keyboard-shortcut first-keystroke="shift alt up" />
  </action>
</keymap>